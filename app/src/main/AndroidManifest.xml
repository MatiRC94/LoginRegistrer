<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.matias.loginregistrer">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="LoginRegister"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
         <activity
             android:name="com.example.matias.loginregistrer.SecondActivity"
             android:label="@string/userarea"
             android:parentActivityName="com.example.matias.loginregistrer.LoginActivity">
         </activity>
     </application>

 </manifest>
